{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Library Analyzer",
            "type": "python",
            "request": "launch",
            "module": "library_analyzer",
            "args": [
                "${input:libraryName}",
                "${input:searchQuery}"
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ],
    "inputs": [
        {
            "id": "libraryName",
            "type": "pickString",
            "description": "Select a library",
            "options": ["openai", "mistralai", "anthropic", "google.generativeai", "litellm", "requests", "numpy", "pandas", "Custom"],
            "default": "openai"
        },
        {
            "id": "searchQuery",
            "type": "pickString",
            "description": "Select a search query",
            "options": ["This automatically handles gzip, deflate and brotli encoded responses", "get", "post", "dataframe manipulation", "Custom"],
            "default": "This automatically handles gzip, deflate and brotli encoded responses"
        },
        {
            "id": "libraryNameCustom",
            "type": "promptString",
            "description": "Enter a custom library name"
        },
        {
            "id": "searchQueryCustom",
            "type": "promptString",
            "description": "Enter a custom search query"
        }
    ]
}
